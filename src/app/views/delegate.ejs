            <% } %>
        <% } %>
    });
</script>

<script type="text/javascript" src="/files/js/divselect.js"></script>
<script type="text/javascript" src="/files/js/apartment.js"></script>
<script>
$(function(){
    $.divselect("#delselect","#inputselect");
});

$(function(){
	
	$.Tipmsg.r=null;
		
	var showmsg=function(msg){
		$.flash(msg);
	}
	
	$(".registerform").Validform({
		tiptype:function(msg,o,cssctl){
			if(!o.obj.is("form")){
				showmsg(msg);
			}
		},
		tipSweep:true,
		ajaxPost:true,
        beforeSubmit: function(form) {
            $('body').loading();
            return true;
        },
		callback:function(data){
			console.log(data.msg);
            $('body').loading('stop');
			if(data.code!=0)  //验证失败
			{
				$.flash(data.msg) ;
			
			}
			else
			{
				$.flash(data.msg) ;
				$('.apartmentform')[0].reset();
			}
		}
	});
})
</script>

