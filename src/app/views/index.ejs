                  <div class="clear"></div>
                  <h2><a href="/apartment/?communityId=<%= val._id %>"><%= val.name %></a><span>￥ <em><%= val.minPrice %></em>/月 起</span></h2>
                  <p><%= val.district.name %>-<%= val.commerseArea.name %></p>
                </li>
              <% }) %>

            </ul>     
        </div><!--tabcon end-->

        <div class="tabcon">
            <ul>
                <% dailyRents.forEach(function(val) { %>  

