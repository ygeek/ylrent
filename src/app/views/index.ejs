<div class="h_products">
    <div class="tabbox" id="statetab">

        <ul class="tabbtn">
            <li class="current"><a>精品房源</a></li>
            <li><a>品质小区</a></li>	
            <li style="border:0;"><a>日租公寓</a></li>	
        </ul><!--tabbtn end-->

        <div class="clear"></div>

        <div class="tabcon">
            <ul>

                <% apartmentTypes.forEach(function(val) { %>

                <li>
                  <a href="/apartment/type/<%= val._id %>">
                    <img class="imghover"
                            <% if (val.imagekeys[0]) { %>
                         src="http://o7k9opgtr.bkt.clouddn.com/<%= val.imagekeys[0] %>?imageView2/0/w/320/h/240"
                            <% }else{ %>
                         src="img/default.jpg"
                            <% } %>
                    />
                  </a>
                  <h1><a href="/apartment/type/<%= val._id %>"><%= val.comunity.name %></a></h1>
                  <div class="clear"></div>
                  <h2><a href="/apartment/type/<%= val._id %>"><%= val.name %></a><span>￥ <em><%= val.minPrice %></em>/月 起</span></h2>
                  <p><%= val.roomType.shi %>室<%= val.roomType.ting %>厅<%= val.roomType.wei %>卫 | <%= val.maxArea %>平米</p>
                  <p class="address"><%= val.district.name %>-<%= val.commerseArea.name %></p>
                </li>
                <% }) %>

            </ul>     
        </div><!--tabcon end-->

        <div class="tabcon">
            <ul>
              <% comunities.forEach(function(val) { %>

                <li>
                  <a href="/apartment/?communityId=<%= val._id %>">
